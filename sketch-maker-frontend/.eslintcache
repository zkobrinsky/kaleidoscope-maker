ht = p5.map(p5.sin(xoff), -1,1,0.3,1);\r\n        xoff += 1;\r\n        \r\n        p5.stroke(hu, sat, light, 1);\r\n\r\n        for (let i = 0; i < symmetry; i++) {\r\n          let angle = 360 / symmetry;\r\n          p5.rotate(angle);\r\n          let d = p5.dist(mx, my, pmx, pmy)\r\n          // let sw = 8\r\n          let sw = p5.map(d, 0, 20, 20, 3) /* variable strokeweight */\r\n          p5.strokeWeight(sw)\r\n          p5.line(mx, my, pmx, pmy);\r\n          p5.push();\r\n          p5.scale(1, -1);\r\n          p5.line(mx, my, pmx, pmy);\r\n          p5.pop();\r\n        }\r\n\r\n    }\r\n    bgAlpha+= 0.00005;\r\n    if (p5.frameCount % 30 == 0) {\r\n      console.log(bgAlpha)\r\n    }\r\n\r\n    if (bgAlpha > 0.02) {\r\n      bgAlpha += 0.02;\r\n    }\r\n    if (bgAlpha > 1) {\r\n      bgAlpha = 0;\r\n    }\r\n  };\r\n\r\n  const windowResized = (p5) => {\r\n    p5.resizeCanvas(p5.windowWidth, p5.windowHeight * 0.915);\r\n    p5.background(...bg);\r\n  };\r\n\r\n\r\n  return <Sketch setup={setup} draw={draw} windowResized={windowResized} />;\r\n};","/mnt/c/Users/ZKobrinsky/dev/flatiron/labs/_projects/p5-sketch-maker/sketch-maker-frontend/src/components/SketchShow.js",["74"],"import React from 'react';\r\nimport { connect } from 'react-redux'\r\n\r\nconst SketchShow = (props) => {\r\n    const sketch = props.sketches.find(sketch => sketch.id == +props.match.params.id)\r\n    return (\r\n        <div>\r\n            <h1>{sketch.title}</h1>\r\n            <img src={sketch.image_full} alt={sketch.title} className=\"photo-medium\" />\r\n        </div>\r\n    )\r\n}\r\n\r\nconst mapStateToProps = ({sketches}) => {\r\n    return ({sketches: sketches.all})\r\n}\r\n\r\nexport default connect(mapStateToProps)(SketchShow)\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n",{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":13,"column":1,"nodeType":"81","endLine":61,"endColumn":3},{"ruleId":"82","severity":1,"message":"